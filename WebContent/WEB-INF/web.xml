<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <servlet>
        <servlet-name>AutoQuote</servlet-name>
        <servlet-class>
            com.altagram.autoquote.AutoQuote
        </servlet-class>
        
        
    </servlet>

    <servlet-mapping>
        <servlet-name>AutoQuote</servlet-name>
        <url-pattern>/getQuote</url-pattern>
               
    </servlet-mapping>
    
    <welcome-file-list>
    	<welcome-file>/Resources/html/AutoQuote.html</welcome-file>  	
    </welcome-file-list>

</web-app>
